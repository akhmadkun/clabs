set / routing-policy policy all default-action policy-result accept
set / interface ethernet-1/1 admin-state enable
set / interface ethernet-1/1 subinterface 0 ipv4 admin-state enable
set / interface ethernet-1/1 subinterface 0 ipv4 address 192.168.11.0/31

set / network-instance default interface ethernet-1/1.0
set / network-instance default interface ethernet-1/2.0
set / network-instance default protocols bgp autonomous-system 300
set / network-instance default protocols bgp router-id 12.12.12.12
set / network-instance default protocols bgp afi-safi ipv4-unicast admin-state enable
set / network-instance default protocols bgp group leafs export-policy [ all ]
set / network-instance default protocols bgp group leafs import-policy [ all ]
set / network-instance default protocols bgp neighbor 192.168.11.1 peer-as 100
set / network-instance default protocols bgp neighbor 192.168.11.1 peer-group leafs
set / network-instance default protocols bgp neighbor 192.168.12.1 peer-as 200
set / network-instance default protocols bgp neighbor 192.168.12.1 peer-group leafs
